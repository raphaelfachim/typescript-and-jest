{
  "name": "ts-and-jest",
  "version": "1.0.0",
  "description": "Projeto base para desenvolvimento com TypeScript e testes com o framework Jest",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "dev:server": "ts-node-dev --respawn --transpile-only --watch 'src/**' src/api/main.ts",
    "dev": "ts-node src/index.ts",
    "server": "ts-node-dev --respawn --transpile-only src/data/exportData/api/index.ts"
  },
  "keywords": [
    "ts",
    "jest"
  ],
  "author": "Raphael Fachim",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/node-fetch": "^2.6.2",
    "@types/pg": "^8.6.5",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "node-fetch": "^3.2.10",
    "pg": "^8.8.0"
  }
}
